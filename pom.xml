			            <phase>prepare-package</phase>
			            <goals>
			             	<goal>copy-resources</goal>
			            </goals>
			            <configuration>
			            	<overwrite>true</overwrite>
			                <outputDirectory>${project.build.directory}/standalone/tomcat/conf</outputDirectory>
			                <resources>          
